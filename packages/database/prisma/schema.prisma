// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Donation {
id          Int      @id @default (autoincrement ())
count       Int
createdAt   DateTime @default (now())
displayName String
email       String
mobile      String?
message     String?
team        String?
}